{
    "id": "Sankey.Chart",
    "version": "1.0.0",
    "supportsMobile": true,
    "supportsLinkedAnalysisFilterOnSelection": true,
    "name": "EChartsSankey",
    "eula": "",
    "vendor": "Poornima",
    "license": "",
    "newInstancePrefix": "EChartsSankey",
    "description": "A sample custom widget project",
    "webcomponents": [
      {
        "kind": "main",
        "tag": "com-sap-sac-sample-echarts-sankeyyg",
        "url": "https://prkerry.github.io/CustomWidget/main.js",
        "integrity": "",
        "ignoreIntegrity": true
      }
    ],
    "properties": {
      "width": {
        "type": "integer",
        "default": 600
      },
      "height": {
        "type": "integer",
        "default": 320
      }
    },
    "events": {},
    "methods": {
          "getDataSource": {
              "parameters": [],
              "body": "return this.dataBindings.getDataBinding('dataBinding').getDataSource();",
              "returnType": "DataSource"
          }
    },
    "types": {},
    "dataBindings": {
      "dataBinding": {
        "feeds": [
          {
            "id": "dimensions",
            "description": "Dimensions",
            "type": "dimension"
          },
          {
            "id": "measures",
            "description": "Measures",
            "type": "mainStructureMember"
          }
        ]
      }
    }
  }